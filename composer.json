{
    "name": "cristianoc72/psr2-code-generator",
    "description" : "Toolset for generating PHP code",
    "type": "library",
    "license": "Apache2",
    "authors": [{
        "name": "Thomas Gossmann",
        "homepage": "http://gos.si"
        },
        {
            "name": "Cristiano Cinotti",
            "email": "cristianocinotti@gmail.com"
        }
    ],
    "keywords": [
        "code generation", "PSR-2"
    ],
    "autoload": {
        "psr-4": {
            "cristianoc72\\codegen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "cristianoc72\\codegen\\tests\\": "tests/"
        }
    },
    "require": {
        "php" : ">=5.6",
        "gossi/docblock" : "~1",
        "symfony/options-resolver" : "^3",
        "phootwork/file" : "~0",
        "phootwork/tokenizer" : "~0",
        "nikic/php-parser" : "^3|^2|^1"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^5.7|^6.0"
    }
}